{
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "np"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "opds-generator",
  "author": "Aki Rautio",
  "module": "dist/opds-generator.esm.js",
  "size-limit": [
    {
      "path": "dist/opds-generator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/opds-generator.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/jest": "^26.0.20",
    "fast-xml-parser": "^3.17.6",
    "husky": "^4.3.7",
    "np": "^7.2.0",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "xmlbuilder2": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
